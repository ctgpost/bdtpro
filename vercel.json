{
  "version": 2,
  "builds": [
    {
      "src": "dist/server/node-build.mjs",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/**"
        ]
      }
    },
    {
      "src": "dist/spa/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/server/node-build.mjs"
    },
    {
      "src": "/health",
      "dest": "dist/server/node-build.mjs"
    },
    {
      "src": "/debug",
      "dest": "dist/server/node-build.mjs"
    },
    {
      "src": "/test",
      "dest": "dist/server/node-build.mjs"
    },
    {
      "src": "/(.*)",
      "dest": "dist/spa/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}